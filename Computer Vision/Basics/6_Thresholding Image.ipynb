    "import cv2\n",
    "import imutils\n",
    "img = cv2.imread(\"keyboard.jpg\")\n",
    "grayImg = cv2.cvtColor(img, cv2.COLOR_BGR2GRAY)\n",
    "thresholdImg = cv2.threshold(grayImg, 60, 255, cv2.THRESH_BINARY)[1]\n",
    "cv2.imwrite(\"Threshold Imgage.jpg\", thresholdImg)"
